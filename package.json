  "description": "Remove unused font glyphs from the project during packaging to reduce font file size",
  "author": "Lete114 <me@imlete.cn>",
  "license": "MIT",
  "homepage": "https://github.com/lete114/rollup-plugin-font-minify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lete114/rollup-plugin-font-minify.git"
  },
  "bugs": {
    "url": "https://github.com/lete114/rollup-plugin-font-minify/issues"
  },
  "exports": {
    ".": {
